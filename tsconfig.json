{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "moduleSuffixes": [
      ".ios",
      ".android",
      ".native",
      ".web",
      ""
    ],
    "paths": {
      "@/configs/*": [
        "configs/*"
      ],
      "@/features/*": [
        "features/*"
      ],
      "@/providers/*": [
        "providers/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/generated/*": [
        "generated/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/constants/*": [
        "constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
